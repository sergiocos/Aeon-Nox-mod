<?xml version="1.0" encoding="utf-8"?>
<includes>
    <default type="image">
        <posx>0</posx>
        <posy>0</posy>
        <texture>-</texture>
        <colorkey>0</colorkey>
        <fadetime>400</fadetime>
        <aspectratio>stretch</aspectratio>
    </default>
    <default type="multiimage">
        <include>FullscreenDimensions</include>
        <timeperimage>7000</timeperimage>
        <randomize>true</randomize>
        <fadetime>800</fadetime>
        <aspectratio align="center">scale</aspectratio>
        <include condition="![Window.IsActive(1115) | Window.IsActive(weather)]">Animation_WindowFade250</include>
    </default>
    <default type="label">
        <posx>0</posx>
        <posy>0</posy>
        <label>-</label>
        <aligny>center</aligny>
        <align>left</align>
        <font>Font_Reg18</font>
        <textcolor>white2</textcolor>
        <selectedcolor>$VAR[ColorDiffuseVar]</selectedcolor>
        <scrollspeed>20</scrollspeed>
    </default>
    <default type="fadelabel">
        <posx>0</posx>
        <posy>0</posy>
        <aligny>center</aligny>
        <align>left</align>
        <font>Font_Reg18</font>
        <textcolor>white2</textcolor>
        <scrollspeed>20</scrollspeed>
        <pauseatend>3000</pauseatend>
        <scrollout>false</scrollout>
        <selectedcolor>$VAR[ColorDiffuseVar]</selectedcolor>
    </default>
    <default type="button">
        <posx>0</posx>
        <posy>0</posy>
        <width>450</width>
        <height>63</height>
        <texturefocus border="5">views/tripanel/listselect_fo.png</texturefocus>
        <texturenofocus border="5">button-nofocus.png</texturenofocus>
        <selectedcolor>$VAR[ColorDiffuseVar]</selectedcolor>
        <font>Font_Reg18</font>
        <pulseonselect>yes</pulseonselect>
        <textcolor>white2</textcolor>
        <disabledcolor>grey3</disabledcolor>
        <focusedcolor>white</focusedcolor>
        <textoffsetx>10</textoffsetx>
        <aligny>center</aligny>
    </default>
    <default type="togglebutton">
        <posx>0</posx>
        <posy>0</posy>
        <width>450</width>
        <height>63</height>
        <pulseonselect>yes</pulseonselect>
        <texturefocus border="5">views/tripanel/listselect_fo.png</texturefocus>
        <texturenofocus border="5">button-nofocus.png</texturenofocus>
        <alttexturefocus border="5">views/tripanel/listselect_fo.png</alttexturefocus>
        <alttexturenofocus border="5">dialogs/context_nofocus.png</alttexturenofocus>
        <font>Font_Reg18</font>
        <textcolor>white2</textcolor>
        <selectedcolor>$VAR[ColorDiffuseVar]</selectedcolor>
        <disabledcolor>grey3</disabledcolor>
        <focusedcolor>white</focusedcolor>
        <textoffsetx>10</textoffsetx>
