<?xml version="1.0" encoding="utf-8"?>
<window type="dialog" id="52">
    <zorder>2</zorder>
    <visible>Window.IsVisible(mutebug) | Window.IsVisible(volumebar)</visible>
    <animation type="WindowOpen" reversible="false">
        <effect type="zoom" start="80" end="100" center="960,540" delay="200" tween="back" time="300" />
        <effect type="fade" delay="200" end="100" time="300" />
    </animation>
    <animation type="WindowClose" reversible="false">
        <effect type="zoom" start="100" end="80" center="960,540" easing="in" tween="back" time="300" />
        <effect type="fade" start="100" end="0" time="300" />
    </animation>
    <controls>
        <control type="group">
            <animation type="Conditional" reversible="false" condition="!Window.IsVisible(volumebar)">
                <effect type="slide" end="976" time="800" delay="3000" tween="cubic" />
            </animation>
            <animation effect="slide" end="0,-870" time="0" condition="True">Conditional</animation>
            <animation effect="zoom" start="100" end="70" center="960,1080" time="800" easing="inout" tween="circle" condition="true">Conditional</animation>
            <posx>808</posx>
            <posy>900</posy>
            <width>764</width>
            <height>136</height>
            <control type="image">
                <posx>-4</posx>
                <posy>-4</posy>
                <width>300</width>
                <height>147</height>
                <colordiffuse>$VAR[ColorDiffuseVar]</colordiffuse>
                <visible>!Skin.HasSetting(DisableGlowbar)</visible>
                <include>PanelGlowFade</include>
                <include>Animation_VisibleChange400</include>
                <texture>dialogs/scan_backg_shadow.png</texture>
            </control>
            <control type="image">
                <posx>-4</posx>
